    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Main",
            "request": "launch",
            "mainClass": "com.example.Main",
            "projectName": "quartz"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "local",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.cicdnexus.MbSpringRestApplication",
            "projectName": "spring-rest",
            "args": "--spring.profiles.active=local"
        },
        {
            "type": "java",
            "name": "dev",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.cicdnexus.MbSpringRestApplication",
            "projectName": "spring-rest",
            "args": "--spring.profiles.active=dev"
        }
    ]
}